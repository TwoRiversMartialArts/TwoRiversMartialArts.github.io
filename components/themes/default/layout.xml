<script type="text/javascript">
    function FillBannerText(outgoing,incoming,opts) {
        // 'this' is the currently displayed slide element
        var cycleId = this.id;
        var imgS = $('#' + cycleId).find('img:visible').attr("src");
        var bannerText = '';
        
        if (imgS.indexOf('main-01') != -1 ) {
            //keyboard
            bannerText = '<h2>Content Management</h2><br />You want to maintain your website yourself?<br />We can help you do that!<br /><br /><a href="{folder-root}/web/quote.aspx" class="button">Get a quote today!</a>';
        } else if (imgS.indexOf('main-02') != -1 ) {
            //pencils
            bannerText = '<h2>Good web design makes great impressions</h2><br />On the internet, first impressions count.<br />We design high quality and functional websites for businesses and non-profit organizations.<br /><br /><a href="{folder-root}/web/services.aspx" class="button">See what we offer!</a>';
        } else if (imgS.indexOf('main-03') != -1 ) {
            //web2.0
            bannerText = '<h2>Web 2.0 finally makes sense to use</h2><br />Web 2.0 has enormous potential and we can help you use it successfully.<br /><br /><a href="{folder-root}/web/services.aspx" class="button">See what we offer!</a>';
        } else if (imgS.indexOf('main-04') != -1 ) {
            //balls
            bannerText = '<h2>We help you succeed on the internet</h2><br />Take advantage of our experience and knowledge.<br /><br /><a href="{folder-root}/web/quote.aspx" class="button">Get a quote today!</a>';
        }
        
        $('#bannerCaption').hide();
        $('#bannerCaption').html(bannerText).fadeIn(1000);
        
        //$('#bannerCaption').html(bannerText)
    }
</script>


